.cf-button  {
  /* padding: 0 10px; */
  display: inline-block;
  height: 30px;
  border: solid 1px transparent;
  padding: 0;
  outline: 0;
  font-weight: 500;
  font-size: 11px;
  text-decoration: none;
  white-space: nowrap;
  word-wrap: normal;
  line-height: normal;
  vertical-align: middle;
  cursor: pointer;
  border-radius: 2px;
  box-shadow: 0 1px 0 rgba(0,0,0,0.05);
}

button:focus {
  outline: none;
  background-color: transparent;
}

