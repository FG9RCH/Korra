body {font: 18px "Lucida Grande", Helvetica, Arial, sans-serif;}

a {
  color: #3b5998;
}

a:active, a:hover {
  outline: 0;
  background-color: transparent;
}

a.sideMenuHover:hover {
  outline: 0;
  background-color: rgb(125,166,218);
  color: white!important;
}

md-backdrop.md-sidenav-backdrop {
  display: none;
}

.adminLogo {
  padding: 0px;
  max-height: 140px;
}

md-sidenav  {
  width: 230px !important;
  height:100%;
  background-color: #6bc7cc;
}

md-content {
  height:100%;
}

md-icon svg {
  fill: inherit;
}

md-icon  {
  fill: inherit;
}

.side-nav {
  height: 100% !important;
}

.adminNav.nav-wrapper {
  position: relative;
  height: 100%;
  padding-left: 25px;
  padding-right: 25px;
}

.side-nav li {
  float: none;
  line-height: 64px;
  padding: 0px 0 0px 0px !important;
}

.side-nav a {
  color: #444;
  display: block;
  font-size: 13px;
  height: 28px;
  line-height: 28px;
  padding: 0 30px;
}

@media only screen and (min-width: 601px){
nav, nav .nav-wrapper i, nav a.button-collapse, nav a.button-collapse i {
  height: 50px;
  line-height: 50px;
}}

#cf-positioner {
  position: fixed;
  top: 0;
  right: 0;
  left: 0;
  -moz-transform: translateZ(0);
  -ms-transform: translateZ(0);
  -webkit-transform: translateZ(0);
  transform: translateZ(0);
  min-height: 91px;
}

